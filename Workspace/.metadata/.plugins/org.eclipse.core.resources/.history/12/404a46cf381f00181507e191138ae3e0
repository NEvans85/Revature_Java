package three;

public class TalkablePerson {
	private String name;
	private int age;
	private void talk() {
		System.out.println("Hello my name is " + name);
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
}
